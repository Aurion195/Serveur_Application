<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web       Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>
<web-app>
	<servlet>
		<servlet-name>HelloWorld</servlet-name>      
		<servlet-class>tm.HelloWorld</servlet-class>
		<init-param>
			<param-name>name</param-name>
			<param-value>Lucien</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>DefaultName</servlet-name>      
		<servlet-class>tm.DefaultName</servlet-class>
	</servlet>

	
	<servlet-mapping>
		<servlet-name>HelloWorld</servlet-name>
		<url-pattern>/HelloWorld</url-pattern>  
	</servlet-mapping> 
	<servlet-mapping>
		<servlet-name>DefaultName</servlet-name>
		<url-pattern>/DefaultName</url-pattern>  
	</servlet-mapping> 
</web-app>